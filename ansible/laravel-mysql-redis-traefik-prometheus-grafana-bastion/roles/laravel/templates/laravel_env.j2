APP_NAME=Laravel
APP_ENV={{ laravel_env }}
APP_KEY={{ laravel_app_key }}
APP_DEBUG={{ laravel_debug | default(false) | bool | ternary('true', 'false') }}
APP_URL={{ app_url }}

VIEW_COMPILED_PATH=/var/www/storage/framework/views

LOG_CHANNEL=stack
#LOG_LEVEL=debug

DB_CONNECTION=mysql
DB_HOST={{ db_host }}
DB_PORT=3306
DB_DATABASE={{ db_name }}
DB_USERNAME={{ db_user }}
DB_PASSWORD={{ db_password }}

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

REDIS_CLIENT=phpredis
REDIS_HOST={{ redis_host }}
REDIS_PASSWORD={{ redis_password }}
REDIS_PORT=6379

CACHE_DRIVER=redis
QUEUE_CONNECTION=redis
SESSION_DRIVER=redis

BROADCAST_DRIVER=log
MAIL_MAILER=log

VITE_APP_NAME="${APP_NAME}"

